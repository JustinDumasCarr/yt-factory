# Gemini API Key for planning and lyrics generation
GEMINI_API_KEY=your_gemini_api_key_here

# Suno API Key for music generation
SUNO_API_KEY=your_suno_api_key_here

# Suno model version (V4, V4_5, V4_5PLUS, V4_5ALL, V5)
# Default: V4_5ALL (better quality, up to 8 min tracks)
SUNO_MODEL=V4_5ALL

# Suno callback URL (required but not used in poll-only mode)
# Default: localhost placeholder
SUNO_CALLBACK_URL=http://localhost/ytf-suno-callback-disabled

# Path to YouTube OAuth credentials JSON file
YOUTUBE_OAUTH_CREDENTIALS_PATH=path/to/credentials.json
